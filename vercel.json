{
  "buildCommand": "npm install pg && npm run build",
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "crons": [
    {
      "path": "/api/loans/process-scheduled-jobs",
      "schedule": "*/1    "
    }
  ]
  }
  